    "@fortawesome/vue-fontawesome": "~3.0.5",
    "@vueuse/core": "^10.7.0",
    "axios": "1.6.2",
    "echarts": "^5.4.3",
    "element-plus": "2.4.3",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.3.10",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
