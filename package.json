    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx,.vue",
    "lint:fix": "eslint --fix --cache --ext .js,.ts,.jsx,.tsx,.vue ./src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
