    "@vue/compiler-sfc": "^3.3.10",
    "@vue/test-utils": "2.4.3",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.19.2",
    "identity-obj-proxy": "^3.0.0",
